{
  "name": "frontend",
  "description": "A production-ready boilerplate built with ES6 React/Redux",
  "license": "MIT",
  "version": "0.0.1",
  "author": "Brian Cappello",
  "homepage": "https://github.com/briancappello",
  "repository": "https://github.com/briancappello/flask-react-spa",
  "scripts": {
    "analyze": "node node_modules/webpack-bundle-analyzer/lib/bin/analyzer.js static/stats.json --port 5556",
    "extract": "NODE_ENV=development extract-messages -l=en,hu -o frontend/app/translations -d en --flat './frontend/app/**/!(*.test).js'",
    "build:analyze": "npm run build && npm run analyze",
    "build:dll": "node frontend/internals/scripts/dependencies.js",
    "build:webpack": "NODE_ENV=production webpack --config frontend/internals/webpack/webpack.prod.config.js",
    "build": "npm run clean && npm run build:webpack",
    "clean": "rimraf static",
    "postinstall": "npm run build:dll",
    "postsetup": "npm run build:dll",
    "start": "node frontend/server",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watchAll"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@brainhubeu/react-carousel": "^1.19.26",
    "@date-io/moment": "^1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^4.0.0-alpha.12",
    "@turf/turf": "^5.1.6",
    "classnames": "^2.2.5",
    "connected-react-router": "^6.4.0",
    "country-flag-icons": "^1.2.8",
    "date-fns": "^2.11.1",
    "hoist-non-react-statics": "^2.3.1",
    "invariant": "^2.2.2",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.4",
    "leaflet": "^1.6.0",
    "leaflet-editable": "^1.2.0",
    "leaflet-geometryutil": "^0.9.3",
    "lodash": "^4.17.19",
    "material-table": "1.67.1",
    "moment": "^2.27.0",
    "normalize.css": "^4.2.0",
    "normalizr": "^3.6.0",
    "notistack": "^1.0.1",
    "path-to-regexp": "^2.0.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.0",
    "react": "^16.13.1",
    "react-big-calendar": "^0.26.0",
    "react-dom": "^16.8.0",
    "react-grid-layout": "^1.2.0",
    "react-helmet": "^5.2.0",
    "react-highlight": "github:briancappello/react-highlight#react-v16-compiled",
    "react-intl": "^5.8.8",
    "react-leaflet-control": "^2.1.2",
    "react-loadable": "^4.0.5",
    "react-redux": "^7.2.2",
    "react-redux-loading-bar": "^2.9.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-sizeme": "^2.6.12",
    "react-slick": "^0.26.1",
    "react-svg": "^2.1.22",
    "redux": "^3.7.2",
    "redux-form": "^8.3.6",
    "redux-orm": "^0.16.2",
    "redux-promising-modals": "^2.1.2",
    "redux-saga": "^0.15.6",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.1",
    "super-skeleton": "github:briancappello/super-skeleton",
    "use-callback-ref": "^1.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-function-bind": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@formatjs/cli": "^2.13.5",
    "add-asset-html-webpack-plugin": "^2.1.1",
    "babel-jest": "^26.6.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-react-intl": "^8.2.10",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "css-loader": "^0.28.5",
    "express": "^4.15.4",
    "express-http-proxy": "^1.6.2",
    "extract-react-intl-messages": "^4.1.1",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "ip": "^1.1.5",
    "jest-cli": "^26.6.1",
    "jest-styled-components": "^7.0.3",
    "json-loader": "^0.5.7",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^4.13.0",
    "react-leaflet": "^2.7.0",
    "redbox-react": "^1.5.0",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.8.4",
    "style-loader": "^0.18.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dllPlugin": {
    "path": "node_modules/flask-api-dlls"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/**/*.test.{js,jsx}",
      "!app/index.js",
      "!app/routes.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/frontend/internals/testing/mocks/cssModule.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/frontend/internals/testing/mocks/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/frontend/internals/testing/test-bundler.js"
    ],
    "testRegex": "tests/.*\\.test\\.js$",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-(native|universal|navigation)-(.*)|@react-native-community/(.*)|@react-navigation/(.*)|bs-platform)"
    ]
  }
}
